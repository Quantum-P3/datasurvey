<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Encuesta.
    -->
    <changeSet id="20210703210921-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="categoria_id"
                                 baseTableName="encuesta"
                                 constraintName="fk_encuesta__categoria_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria"/>

        <addForeignKeyConstraint baseColumnNames="usuario_extra_id"
                                 baseTableName="encuesta"
                                 constraintName="fk_encuesta__usuario_extra_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario_extra"/>
    </changeSet>
</databaseChangeLog>
